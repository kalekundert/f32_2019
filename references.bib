	month = oct,
	year = {2017},
	pages = {26--43},
	file = {ScienceDirect Full Text PDF:/home/kale/.mozilla/firefox/kfgk8o2d.default/zotero/storage/BXXVJ694/Hess et al. - 2017 - Methods and Applications of CRISPR-Mediated Base E.pdf:application/pdf;ScienceDirect Snapshot:/home/kale/.mozilla/firefox/kfgk8o2d.default/zotero/storage/AVH9F926/S1097276517307074.html:text/html}
}

