	keywords = {Cell fusion, Cloning, Flow cytometry, DNA recombination, Cell cycle and cell division, Non-homologous end joining, Small interfering RNAs, Synthesis phase},
	pages = {e16501},
	file = {Full Text PDF:/home/kale/.mozilla/firefox/kfgk8o2d.default/zotero/storage/H7UFZDAW/Hartlerode et al. - 2011 - Cell Cycle-Dependent Induction of Homologous Recom.pdf:application/pdf;Snapshot:/home/kale/.mozilla/firefox/kfgk8o2d.default/zotero/storage/4FF7RQH3/article.html:text/html}
